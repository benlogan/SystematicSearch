	doi = {https://doi.org/10.1016/j.future.2015.02.009},
	issn = {0167-739X},
	journal = {Future Generation Computer Systems},
	keywords = {Cloud computing, Economics, IT architecture},
	pages = {403-427},
	slr = {YES},
