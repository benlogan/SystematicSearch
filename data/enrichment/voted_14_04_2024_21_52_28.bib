	keywords = {Cloud computing, Renewable energy, Firm-specific advantages, Country-specific advantages, Home countries, Host countries},
	pages = {125501},
	title = {The Cloud's fearsome five renewable energy strategies: Coupling sustainable development goals with firm specific advantages},
