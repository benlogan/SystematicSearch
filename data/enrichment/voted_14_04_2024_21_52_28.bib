	location = {Markham, Ontario, Canada},
	numpages = {10},
	pages = {257--266},
	publisher = {IEEE Press},
	series = {ESEM '17},
	title = {Estimating Energy Impact of Software Releases and Deployment Strategies: The KPMG Case Study},
